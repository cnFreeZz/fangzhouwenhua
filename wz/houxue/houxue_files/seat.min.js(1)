var v_ulid=0;var v_refer=encodeURIComponent(document.referrer);var v_cururl=encodeURIComponent(window.location.href);var v_webtitle=encodeURIComponent(document.title);var v_loginid=0;var v_courseid=0;if(getMLoginCookie("userid")){v_ulid=getMLoginCookie("userid")}if(document.getElementsByName("loginid").length>0){v_loginid=document.getElementsByName("loginid")[0].value}if(v_loginid<=0){var sid=v_cururl.match(/\/xuexiao-(\d+).html/);if(sid){v_loginid=sid[1]}}var id=v_cururl.match(/\/kecheng-(\d+).html/);if(id){v_courseid=id[1]}$(".hx_zx_click").on("click",function(){getSeatAndOpen(v_loginid,v_courseid)});function openchat(q,d,m,u,r,j,f,t,a,k,l,o,b,i,p){if(!q||!m||!l){return false}v_loginid=o;v_courseid=b;if(v_courseid<=0&&v_loginid<=0){v_webtitle=""}var n=680;var e=530;var h=(window.screen.availHeight-30-e)/2;var c=(window.screen.availWidth-10-n)/2;var s={c:l,s:k,pg:v_courseid,sn:"",w:q,r:v_refer,cururl:v_cururl,n:encodeURIComponent(m),p:u,si:r,sl:v_loginid,st:j,phone:f,comname:encodeURIComponent(t),webtitle:v_webtitle,ulid:v_ulid,cellphone:"",username:"",seattype:i,aid:a};if(d==0){if(p==1){var g="https://im.houxue.com/chat/robot/robotchatbox?"+$.param(s)}else{var g="https://im.houxue.com/chat/message/index?"+$.param(s)}}else{var g="https://im.houxue.com/chat/talk/enter?"+$.param(s)}if(typeof(chat_open)=="function"&&v_loginid!=196115){chat_open(k,g)}else{window.open(g,k,"height="+e+", innerHeight="+e+", width="+n+", innerWidth="+n+", top="+h+", left="+c+", toolbar=no, menubar=no, scrollbars=no, resizable=no, location=yes, status=no")}}function getSeatAndOpen(f,e){var l="";var o="";var b="";var a="";var g="";var q="";var i="";var d="";var k=0;var m=0;var h=0;var r="";var c=0;var j={loginid:f,courseid:e};$.ajax({url:"/newseat/index",type:"post",dataType:"json",async:false,data:j,success:function(p){var n=p.content;if(n){l=n.w;o=n.t;b=n.n;a=n.p;g=n.si;q=n.st;if(n.appid){k=n.appid}i=n.phone;d=n.comname;m=n.siteid;r=n.cusid;h=n.seattype;c=n.robotflag}}});if(m>0){openchat(l,o,b,a,g,q,i,d,k,m,r,f,e,h,c)}else{if($(".qiao-invite-accept").length>0){$(".qiao-invite-accept").click()}else{if($("#QIAO_ICON_CONTAINER").length>0){$("#QIAO_ICON_CONTAINER").click()}}}}function getMLoginCookie(a){if(document.cookie.length>0){c_start=document.cookie.indexOf(a+"=");if(c_start!=-1){c_start=c_start+a.length+1;c_end=document.cookie.indexOf(";",c_start);if(c_end==-1){c_end=document.cookie.length}return unescape(document.cookie.substring(c_start,c_end))}}return""};
